{
  "name": "h2o-flow",
  "version": "0.0.0",
  "description": "Interactive computational environment for H2O",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm install && bower install && gulp",
      "build": "mkdir -p build/css && mkdir -p build/custom &&  cp -a src/custom/. build/custom && mkdir -p build/fonts && cp -a src/fonts/. build/fonts && mkdir -p build/img && cp -a src/img/. build/img && mkdir -p build/js && gulp",
    "clean": "gulp clean",
    "watch": "gulp && gulp watch",
    "headless": "gulp && phantomjs build/js/headless-test.js --packs examples",
    "test-raw": "gulp build-tests && node build/js/flow-tests.js -s",
    "test": "gulp build-tests && node build/js/flow-tests.js -s | faucet",
    "coverage": "mkdir -p build/js/coverage && gulp build-tests && istanbul cover --dir build/js/coverage -x \"**/lib/**\" build/js/flow-tests.js && istanbul report --dir build/js/coverage cobertura"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h2oai/h2o-flow"
  },
  "keywords": [
    "H2O",
    "Machine Learning"
  ],
  "author": "Prithvi Prabhu <prithvi@h2o.ai>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h2oai/h2o/issues"
  },
  "homepage": "https://github.com/h2oai/h2o-flow",
  "devDependencies": {
    "async": "~0.9.0",
    "bower": "~1.3.8",
    "browserify": "~3.44.2",
    "coffee-script": "~1.7.1",
    "coffeelint": "~1.1.0",
    "desugar": "0.0.3",
    "escodegen": "~1.4.1",
    "esprima": "~1.2.2",
    "faucet": "0.0.1",
    "groc": "~0.6.3",
    "gulp": "~3.8.2",
    "gulp-clean": "~0.3.1",
    "gulp-coffee": "~2.0.1",
    "gulp-concat": "~2.2.0",
    "gulp-footer": "~1.0.4",
    "gulp-header": "~1.0.2",
    "gulp-if": "~1.2.1",
    "gulp-ignore": "~1.1.0",
    "gulp-jade": "~0.6.0",
    "gulp-order": "~1.1.1",
    "gulp-stylus": "~1.0.2",
    "gulp-task-listing": "~0.3.0",
    "gulp-util": "~2.2.18",
    "istanbul": "~0.2.7",
    "jade": "~1.3.1",
    "js-yaml": "~3.2.2",
    "jshint": "~2.5.0",
    "lodash": "~2.4.1",
    "marked": "~0.3.2",
    "minimist": "~1.1.0",
    "nib": "~1.0.2",
    "request": "~2.34.0",
    "stylus": "~0.43.0",
    "tape": "~2.12.3",
    "through2": "~0.6.1",
    "uglify-js": "~2.4.13",
    "watchy": "~0.4.3"
  },
  "dependencies": {}
}
