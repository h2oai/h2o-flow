  "scripts": {
    "build": "webpack --mode production",
    "clean": "rm -r build",
    "watch": "webpack --watch --mode development",
    "start": "webpack-dev-server --open --mode development",
    "test-raw": "coffee test/unit.tests.coffee -s",
    "test": "coffee test/unit.tests.coffee -s | faucet",
    "headless": "coffee test/headless-test.coffee --host localhost:8080 --packs test-small",
    "headless-compile": "coffee --compile --output build/js test/*-test.coffee"
  },
