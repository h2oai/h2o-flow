    i.fa.fa-cloud-upload
    span(data-bind='text:title')
  table.flow-form
    tbody
      tr
        th.flow-narrow Files
        td.flow-wide
          div(data-bind="template: { name:templateOf, foreach:importViews }")
      // ko if:canParse
      tr.flow-actions
        th.flow-narrow Actions
        td.flow-wide
          +button('Parse these files&hellip;', 'cog', 'parse', 'canParse')
