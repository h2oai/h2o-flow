.flow-widget.flow-timeline-output
  h3.flow-hint
    i.fa.fa-clock-o
    span Timeline

  .flow-panel
    button.flow-button(type='button' data-bind="click:toggleRefresh, css:{ 'flow-button-toggle':isLive }")
      // ko if:isLive
      i.fa.fa-refresh.fa-spin
      | Stop
      // /ko
      // ko ifnot:isLive
      i.fa.fa-refresh
      | Refresh
      // /ko

  .flow-plot(data-bind='raw:data')
